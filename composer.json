{
    "name": "hokan22/laravel-translator",
    "type": "library",
    "description": "Flexible Laravel translator",
    "keywords": [
        "hokan22",
        "laravel",
        "language",
        "multilang",
        "translator"
    ],
    "homepage": "https://github.com/hokan22/laravel-translator",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander Viertel",
            "email": "git@aviertel.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "~5.0",
        "illuminate/console": "~5.0",
        "illuminate/database": "~5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "orchestra/testbench": "~3.4"
    },
    "autoload": {
        "psr-4": {
            "Hokan22\\LaravelTranslator\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    }
}
